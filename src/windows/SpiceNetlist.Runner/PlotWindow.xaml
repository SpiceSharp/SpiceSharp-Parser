<Window x:Class="SpiceNetlist.Runner.PlotWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpiceNetlist.Runner"
        xmlns:oxy="clr-namespace:OxyPlot.Wpf;assembly=OxyPlot.Wpf" 
        mc:Ignorable="d"
        Title="PlotWindow" Height="300" Width="300">
    <DockPanel>
        <Label DockPanel.Dock="Top" Content="Options" />
        <CheckBox Name="x" Content="X-axis Log scale" Height="20" DockPanel.Dock="Top" Click="CheckBox_Click" />
        <CheckBox Name="y" Content="Y-axis Log scale" Height="20" DockPanel.Dock="Top" Click="CheckBox_Click_1"/>
        <oxy:PlotView Name="plot" Model="{Binding Model}" HorizontalAlignment="Stretch" VerticalContentAlignment="Stretch" />
    </DockPanel>
</Window>
